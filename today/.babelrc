{
    "presets": ["react-native"],
    "plugins": [
        [
            "module-resolver",
            {
                "cwd": "babelrc",
                "root": ["./src"],
                "extensions": [".js", ".ios.js", ".android.js"],
                "alias": {
                    "today-modules": "./src/modules",
                    "selectors": "./src/modules/redux/selectors",
                    "types": "./src/modules/redux/types"
                }
            }
        ]
    ]
}
